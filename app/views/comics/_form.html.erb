<%= form_with model: comic do |form| %>
  <% if form.object.errors.any? %>
    <p class="error"><%= form.object.errors.full_messages.first %></p>
  <% end %>

  <div>
    <%= form.label :series %>
    <%= form.text_field :series %>
    <%= form.label :title %>
    <%= form.text_field :title %>
    
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>
